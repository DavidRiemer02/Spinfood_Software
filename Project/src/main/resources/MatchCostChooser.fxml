<GridPane hgap="5.0" prefHeight="195.0" prefWidth="200.0" vgap="5.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.view.windows.MatchCostChooser">
